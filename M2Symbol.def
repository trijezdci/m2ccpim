DEFINITION MODULE M2Symbol;

(* Symbol Definition for Modula-2 R10 Core Compiler *)

FROM M2Token IMPOPRT Token;
FROM LexTab IMPOPRT Lexeme;
FROM M2Diag IMPOPRT Diagnostic;


(* Symbol to be returned by lexer *)

TYPE Symbol = RECORD
  token : Token;
  line, col : CARDINAL;
  lexeme : Lexeme;
  diagnostic : Diagnostic
END; (* Symbol *)


END M2Symbol.